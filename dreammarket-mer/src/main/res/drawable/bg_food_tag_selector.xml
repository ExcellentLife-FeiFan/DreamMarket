<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="#fff1f1f2" />
            <corners android:radius="@dimen/stroke_rect_corner_radius" />
            <stroke android:width="@dimen/stroke_rect_width" android:color="#ffd8d8d8" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/white" />
            <corners android:radius="@dimen/stroke_rect_corner_radius" />
            <stroke android:width="@dimen/stroke_rect_width" android:color="@color/green" />
        </shape>
    </item>
</selector>