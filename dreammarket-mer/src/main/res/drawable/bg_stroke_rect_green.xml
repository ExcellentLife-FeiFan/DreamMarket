<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <solid android:color="#ffcdf3ef" />
            <corners android:radius="@dimen/stroke_rect_corner_radius" />
            <stroke android:width="@dimen/stroke_rect_width" android:color="#ff5fc8b8" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent" />
            <corners android:radius="@dimen/stroke_rect_corner_radius" />
            <stroke android:width="@dimen/stroke_rect_width" android:color="#ffc5c5c5" />
        </shape>
    </item>
</selector>