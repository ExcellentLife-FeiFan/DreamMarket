<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg"
    android:orientation="vertical">

    <include layout="@layout/layout_mytoolbar"></include>


    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="@color/white_background"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/default_ui_margin_large"
                android:background="@color/white"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/ll_spm_1"
                    style="@style/list_item_layout">

                    <TextView
                        android:id="@+id/tv_spm_1"
                        style="@style/left_text_layout"
                        android:text="营业状态" />

                    <TextView
                        style="@style/right_text_layout"
                        android:text="@string/null_s" />
                </LinearLayout>

                <include layout="@layout/commom_line" />

                <LinearLayout
                    android:id="@+id/ll_spm_2"
                    style="@style/list_item_layout">

                    <TextView
                        style="@style/left_text_layout"
                        android:text="配送范围" />

                    <TextView
                        style="@style/right_text_layout"
                        android:text="@string/null_s" />
                </LinearLayout>
                <include layout="@layout/commom_line" />

                <LinearLayout
                    android:id="@+id/ll_spm_3"
                    style="@style/list_item_layout">

                    <TextView
                        style="@style/left_text_layout"
                        android:text="超市名称" />

                    <TextView
                        style="@style/right_text_layout"
                        android:text="@string/null_s" />
                </LinearLayout>
            </LinearLayout>


            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/default_ui_margin_large"
                android:background="@color/white"
                android:orientation="vertical">
                <LinearLayout
                    android:id="@+id/ll_spm_4"
                    style="@style/list_item_layout">

                    <TextView
                        style="@style/left_text_layout"
                        android:text="联系人名称" />

                    <TextView
                        style="@style/right_text_layout"
                        android:text="@string/null_s" />
                </LinearLayout>

                <include layout="@layout/commom_line" />
                <LinearLayout
                    android:id="@+id/ll_spm_5"
                    style="@style/list_item_layout">

                    <TextView
                        style="@style/left_text_layout"
                        android:text="绑定手机号" />

                    <TextView
                        android:id="@+id/txt_poi_info_poi_phone"
                        style="@style/right_text_layout"
                        android:text="@string/null_s" />
                </LinearLayout>

                <include layout="@layout/commom_line" />

                <LinearLayout
                    android:id="@+id/ll_spm_6"
                    style="@style/list_item_layout">

                    <TextView
                        style="@style/left_text_layout"
                        android:text="超市地址" />

                    <TextView
                        style="@style/right_text_layout"
                        android:text="@string/null_s" />
                </LinearLayout>

                <include layout="@layout/commom_line" />

                <LinearLayout
                    android:id="@+id/ll_spm_7"
                    style="@style/list_item_layout">

                    <TextView
                        style="@style/left_text_layout"
                        android:text="@string/title_poi_info_open_time" />

                    <TextView
                        style="@style/right_text_layout"
                        android:text="@string/null_s" />
                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/default_ui_margin_large"
                android:background="@color/white"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/ll_spm_8"
                    style="@style/list_item_layout">

                    <TextView
                        style="@style/left_text_layout"
                        android:text="营业资质" />

                    <TextView
                        style="@style/right_text_layout"
                        android:text="@string/null_s" />
                </LinearLayout>

                <include layout="@layout/commom_line" />

                <LinearLayout
                    android:id="@+id/ll_spm_9"
                    style="@style/list_item_layout">

                    <TextView
                        style="@style/left_text_layout"
                        android:text="配送信息" />

                    <TextView
                        style="@style/right_text_layout"
                        android:text="@string/null_s" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/default_ui_margin_large"
                android:layout_marginTop="@dimen/default_ui_margin_large"
                android:background="@color/white"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/ll_spm_10"
                    style="@style/list_item_layout">

                    <TextView
                        style="@style/left_text_layout"
                        android:text="商家协议" />

                    <TextView
                        style="@style/right_text_layout"
                        android:text="@string/null_s" />
                </LinearLayout>

            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</LinearLayout>